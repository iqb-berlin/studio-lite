  "scripts": {
    "start-app": "nx run-many --target=serve --projects=api,frontend --parallel",
    "start-frontend": "nx serve frontend",
    "start-backend": "nx serve api",
    "test-e2e-live": "nx e2e frontend-e2e --watch",
    "test-e2e": "nx e2e frontend-e2e --prod --browser chromium",
    "test-e2e-api": "nx e2e frontend-e2e --prod --browser chromium --spec \"./apps/frontend-e2e/src/e2e/api/**/*\"",
    "test-e2e-ui": "nx e2e frontend-e2e --prod --browser chromium --spec \"./apps/frontend-e2e/src/e2e/ui/**/*\"",
    "test-e2e-ui-mobile": "nx e2e frontend-e2e --prod --browser chromium --spec \"./apps/frontend-e2e/src/e2e/ui/**/*\" --config \"viewportWidth=375,viewportHeight=667\"",
    "test-app": "nx run-many --target=test --all --parallel",
    "test-frontend": "nx test frontend",
    "test-backend": "nx test api",
    "lint-app": "nx run-many --target=lint --all --parallel",
    "lint-frontend": "nx lint frontend --fix=true",
    "lint-backend": "nx lint api",
    "build-app": "nx run-many --target=build --all --parallel",
    "build-frontend": "nx build frontend",
    "build-backend": "nx build api",
    "ng": "nx"
  },
