  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "build-backend": "nx build api",
    "test-backend": "nx test api",
    "lint-backend": "nx lint api",
    "start-backend": "nx serve api",
    "build-frontend": "nx build frontend",
    "test-frontend": "nx test frontend",
    "lint-frontend": "nx lint frontend",
    "start-frontend": "nx serve frontend",
    "test-e2e-ui-mobile": "nx e2e frontend-e2e --prod --browser chromium --spec \"./apps/frontend-e2e/src/integration/ui/*\" --config \"viewportWidth=375,viewportHeight=667\"",
    "test-e2e-ui": "nx e2e frontend-e2e --prod --browser chromium --spec \"./apps/frontend-e2e/src/integration/ui/*\"",
    "test-e2e-api": "nx e2e frontend-e2e --prod --browser chromium --spec \"./apps/frontend-e2e/src/integration/api/*\"",
    "build-app": "nx run-many --target=build --all --parallel",
    "test-app": "nx run-many --target=test --all --parallel",
    "lint-app": "nx run-many --target=lint --all --parallel",
    "test-e2e-app": "nx e2e frontend-e2e --prod --browser chromium",
    "start-app": "nx run-many --target=serve --projects=api,frontend --parallel"
  },
